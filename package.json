{
  "name": "sampleapi",
  "version": "3.0.0",
  "description": "An OAuth secured API using AWS Serverless Hosting and Lambda Functions",
  "scripts": {
    "lint": "npx eslint . --ext .ts,.tsx",
    "build": "rimraf dist && tsc --sourceMap true",
    "wiremock": "wiremock --root-dir test/integration --port 80",
    "test": "mocha -r ts-node/register test/apiTests.ts",
    "package": "npm run buildRelease && rimraf .serverless && sls package --stage deployed",
    "buildRelease": "rimraf dist && tsc",
    "deploy": "sls deploy --stage deployed --package .serverless --aws-profile default",
    "remove": "sls remove --stage deployed"
  },
  "author": "Gary Archer",
  "license": "MIT",
  "dependencies": {
    "@middy/core": "^2.5.3",
    "axios": "^0.23.0",
    "fs-extra": "^9.0.1",
    "guid-typescript": "^1.0.9",
    "inversify": "^5.0.1",
    "jose": "^4.6.0",
    "js-sha256": "^0.9.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.85",
    "@types/fs-extra": "^9.0.4",
    "@types/mocha": "^9.1.0",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "child-process": "^1.0.2",
    "eslint": "^8.12.0",
    "mocha": "^9.2.2",
    "proxy-agent": "^5.0.0",
    "rimraf": "^3.0.2",
    "serverless": "^3.0.1",
    "serverless-domain-manager": "4.1.0",
    "ts-node": "^10.7.0",
    "typescript": "4.5.5",
    "wiremock": "^2.32.0"
  }
}
