{
    "api": {
        "useProxy": false,
        "proxyUrl": "http://127.0.0.1:8888"
    },
    "logging": {
        "apiName": "SampleApi",
        "prettyPrint": true,
        "performanceThresholdMilliseconds": 500
    },
    "oauth": {
        "issuer":           "https://login.authsamples-dev.com",
        "audience":         "api.mycompany.com",
        "scope":            "https://api.authsamples.com/investments",
        "jwksEndpoint":     "http://login.authsamples-dev.com/.well-known/jwks.json"
    },
    "cache": {
        "isActive": false,
        "region": "eu-west-2",
        "tableName": "OAUTH_CACHE",
        "claimsCacheTimeToLiveMinutes": 15
    }
}
