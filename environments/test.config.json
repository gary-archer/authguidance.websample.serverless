{
    "api": {
        "useProxy": true,
        "proxyUrl": "http://127.0.0.1:8888"
    },
    "logging": {
        "apiName": "SampleApi",
        "performanceThresholdMilliseconds": 500
    },
    "oauth": {
        "issuer":           "testissuer.com",
        "audience":         "api.mycompany.com",
        "jwksEndpoint":     "http://login.mycompany.com/.well-known/jwks.json",
        "userInfoEndpoint": "http://login.mycompany.com/oauth2/userInfo"
    },
    "cache": {
        "region": "eu-west-2",
        "tableName": "OAUTH_CACHE",
        "claimsCacheTimeToLiveMinutes": 30
    }
}
